  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.7.5",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.7.0",
    "eslint-config-customink": "2.1.1",
    "eslint-plugin-react": "4.3.0",
    "expect": "1.16.0",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "signalerjs": "0.4.0",
    "webpack": "1.12.14"
