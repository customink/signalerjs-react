{
  "name": "signalerjs-react",
  "version": "1.1.0",
  "description": "signalerjs React component for A/B testing",
  "main": "dist/signalerjs-react.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "lint": "$(npm bin)/eslint src/**/*.js test/**/*.js",
    "test": "$(npm bin)/jest",
    "preversion": "yarn install && npm test && npm run lint",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/customink/signalerjs-react.git"
  },
  "bugs": {
    "url": "https://github.com/customink/signalerjs-react/issues"
  },
  "keywords": [
    "a/b testing",
    "javascript",
    "feature testing",
    "react",
    "component"
  ],
  "files": [
    "dist"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.7.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^21.2.0",
    "jest": "^21.2.0",
    "jest-cli": "^21.2.0",
    "jsdom": "^11.2.0",
    "mocha": "^4.0.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "signalerjs": "^0.4.0",
    "sinon": "^4.0.1",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "signalerjs": "^0.4.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
