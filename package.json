{
  "name": "signalerjs-react",
  "version": "1.0.0",
  "description": "signalerjs React component for A/B testing",
  "main": "dist/signalerjs-react.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "lint": "$(npm bin)/eslint src/**/*.js test/**/*.js",
    "test": "$(npm bin)/mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "preversion": "yarn install && npm test && npm run lint",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/customink/signalerjs-react.git"
  },
  "bugs": {
    "url": "https://github.com/customink/signalerjs-react/issues"
  },
  "keywords": [
    "a/b testing",
    "javascript",
    "feature testing",
    "react",
    "component"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "jsdom": "^10.0.0",
    "mocha": "^3.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "signalerjs": "^0.4.0",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "signalerjs": "^0.4.0"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  }
}
