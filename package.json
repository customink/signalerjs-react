{
  "name": "signalerjs-react",
  "version": "1.0.0",
  "description": "signalerjs React component for A/B testing",
  "main": "dist/signalerjs-react.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "lint": "$(npm bin)/eslint src/**/*.js test/**/*.js",
    "test": "$(npm bin)/mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "preversion": "yarn install && npm test && npm run lint",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/customink/signalerjs-react.git"
  },
  "bugs": {
    "url": "https://github.com/customink/signalerjs-react/issues"
  },
  "keywords": [
    "a/b testing",
    "javascript",
    "feature testing",
    "react",
    "component"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^2.7.1",
    "eslint": "^4.7.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^1.20.2",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "signalerjs": "^0.4.0",
    "sinon": "^1.17.7",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "signalerjs": "^0.4.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
